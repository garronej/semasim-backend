{
  "name": "manager-page",
  "version": "0.1.0",
  "main": "./dist/manager.js",
  "scripts": {
    "build": "npm run tsc && npm run browserify",
    "tsc": "node ./node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "browserify": "node ./node_modules/browserify/bin/cmd . -o ../../static/manager.js",
    "watchify": "node ./node_modules/watchify/bin/cmd . -o ../../static/manager.js",
    "tsc-w": "node ./node_modules/typescript/bin/tsc -w -p ./tsconfig.json",
    "watch": "sh ./watch.sh"
  },
  "browserify": {
    "transform": [
      "html2js-browserify",
      "lessify"
    ]
  },
  "dependencies": {
    "ts-events-extended": "github:garronej/ts-events-extended"
  },
  "devDependencies": {
    "@types/bootstrap": "3.3.28",
    "@types/icheck": "^0.8.29",
    "@types/jquery": "2.0.48",
    "@types/jquery.validation": "1.13.27",
    "@types/jqueryui": "1.9.27",
    "browserify": "^14.4.0",
    "html2js-browserify": "^1.3.0",
    "lessify": "^1.0.1",
    "typescript": "^2.5.3",
    "watchify": "^3.9.0"
  }
}
